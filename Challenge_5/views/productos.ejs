<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <table>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Price</th>
      <th>Imagen</th>
    </tr>
    <% if (isEmpty) { %>
      <tr>
        <td>No hay Porductos Registrados</td>
      </tr>
    <% } %>
    <% personas.forEach(function(usr) { %>
      <tr>
        <td><%= usr.nombre %></td>
        <td><%= usr.apellido %></td>
        <td><%= usr.edad %></td>
      </tr>
    <% }); %>
  </table>
</body>
</html>